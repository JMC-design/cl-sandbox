#!/bin/sh
#|-*- mode:lisp -*-|#
#| Common Lisp sandbox package for testing out stuff.
exec ros -Q -- $0 "$@"
|#
(eval-when (:compile-toplevel :load-toplevel :execute)
  (ql:quickload '(:cl-sandbox/tests) :silent t))
(defun main (&rest argv)
  (cl-sandbox/tests:run-tests))
